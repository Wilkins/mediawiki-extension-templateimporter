{
	"@Note": "When updating this file please also update PageForms.php with the same changes.",
	"name": "Template Importer",
	"version": "0.1.0",
	"author": [
		"Thibault Taillandier"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Template importer",
	"descriptionmsg": "templateimporter-desc",
	"license-name": "GPL-2.0+",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.27.0"
	},
	"MessagesDirs": {
		"TemplateImporter": [
			"i18n"
		]
	},
	"AutoloadClasses": {
      "BaseImporter": "src/BaseImporter.php",
      "Exception": "src/Exception.php",
      "NamespaceManager": "src/NamespaceManager.php",
      "Page": "src/Page.php",
      "BaseSpecialImportPages": "src/BaseSpecialImportPages.php"
	},
	"@ResourceModules": {
