			"scripts": [
				"libs/PageForms.js",
				"libs/PF_preview.js"
			],
			"styles": [
				"skins/PageForms.css",
				"skins/PF_jquery_ui_overrides.css"
			],
			"dependencies": [
				"jquery.ui.core",
				"jquery.ui.autocomplete",
				"jquery.ui.button",
				"jquery.ui.sortable",
				"jquery.ui.widget",
				"ext.pageforms.fancybox",
				"ext.pageforms.autogrow",
				"mediawiki.util",
				"ext.pageforms.select2"
			],
			"messages": [
				"pf_formerrors_header",
				"pf_too_few_instances_error",
				"pf_too_many_instances_error",
				"pf_blank_error",
				"pf_not_unique_error",
				"pf_bad_url_error",
				"pf_bad_email_error",
